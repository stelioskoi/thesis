@using System.Web.Mvc
@using WebApplication2.Models
@model IEnumerable<WebApplication2.Models.Images>

 
@{
    ViewBag.Title = "Partners";
    
}

<h2>Partners</h2>
<div>

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Image1)
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                  
                    <img src="data:Images;base64,@System.Convert.ToBase64String(item.Image1)" />
                    
                </td>
            </tr>
        }
        </table>
</div>
